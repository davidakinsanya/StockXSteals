CREATE TABLE DailySearchHistory (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    timestamp TEXT NOT NULL,
    name TEXT NOT NULL,
    image TEXT NOT NULL,
    json TEXT NOT NULL
);

getSearchHistory:
SELECT *
FROM DailySearchHistory;

deleteSearch:
DELETE FROM
DailySearchHistory
WHERE id = :id;

addSearch:
INSERT INTO
DailySearchHistory (timestamp, name, image, json)
VALUES (?,?,?,?);

clearSearches:
DELETE FROM DailySearchHistory;
