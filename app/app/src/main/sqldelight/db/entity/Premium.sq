CREATE TABLE premium (
 id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
 isPremium INTEGER NOT NULL
);

getPremiumQuota:
SELECT * FROM premium;

getIsPremium:
SELECT isPremium FROM premium WHERE id = :id;

